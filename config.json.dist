	"username": "",
	"__comment4": "Token from your factorio.com profile page. Required for MP and mod manager services.",
	"token": "",

	"__comment5": "General user settings",
	"verify_user_identity": true,
	"auto_pause": false,
	"allow_commands": "admins-only",
	"game_password": "",
