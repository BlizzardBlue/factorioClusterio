	"databaseDirectory": "./database",
	
	"__commentMasterPort": "HTTP port for the master to listen to",
	"masterPort": 8080,
	
	"__commentsslPort": "HTTPS port for the master to listen to. Set to 0 or an empty string to not listen for HTTPS.",
